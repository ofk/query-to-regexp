  "devDependencies": {
    "@ofk/eslint-config-recommend": "^2.1.0",
    "@types/jest": "^26.0.20",
    "eslint": "^7.21.0",
    "eslint-plugin-jest": "^24.2.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-package-json": "^2.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "keywords": [],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": "prettier-package-json --write",
    "*.{md,json,html}": "prettier --write",
